{
  "name": "xml-js",
  "version": "1.6.11",
  "description": "A convertor between XML text and Javascript object / JSON text.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashwaan/xml-js.git"
  },
  "author": "Yousuf Almarzooqi <ysf953@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashwaan/xml-js/issues"
  },
  "homepage": "https://github.com/nashwaan/xml-js#readme",
  "keywords": [
    "XML",
    "xml",
    "js",
    "JSON",
    "json",
    "cdata",
    "CDATA",
    "doctype",
    "processing instruction",
    "Javascript",
    "js2xml",
    "json2xml",
    "xml2js",
    "xml2json",
    "transform",
    "transformer",
    "transforming",
    "transformation",
    "convert",
    "convertor",
    "converting",
    "conversion",
    "parse",
    "parser",
    "parsing"
  ],
  "main": "lib/index.js",
  "bin": "./bin/cli.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "debug": "nodemon --inspect --watch lib/ --watch test/ --debug-brk test/index.js",
    "debug:cli": "nodemon --inspect --watch lib/ --debug-brk index.js -- --help",
    "jest": "jest --config=test/jest.conf.js --runInBand --detectOpenHandles",
    "open-help": "biased-opener --help",
    "deploy": "npm-run-all --serial git:*",
    "prepublish": "npm run test",
    "test": "npm run jest && npm run test:types",
    "test:types": "tsc -p ./types"
  },
  "dependencies": {
    "sax": "^1.4.3",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "biased-opener": "^0.2.8",
    "browser-sync": "^3.0.4",
    "cash-cat": "^0.2.0",
    "codacy-coverage": "^3.4.0",
    "codeclimate-test-reporter": "^0.5.1",
    "codecov": "^3.8.3",
    "coveralls": "^3.1.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "globify": "^3.0.3",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "jest-mock-process": "^2.0.0",
    "jsonpath": "^1.1.1",
    "mock-stdin": "^1.0.0",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.30.0",
    "typescript": "^5.9.3",
    "unminified-webpack-plugin": "^3.0.0",
    "watch": "^1.0.2",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
